{
	"name": "@medici-uy/ui",
	"version": "0.0.42",
	"description": "UI kit for medici.uy.",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./reset.css": "./dist/reset.css"
	},
	"scripts": {
		"build": "swc src --out-dir dist --copy-files src/*.css --strip-leading-paths",
		"prebuild": "rm --recursive --force dist && npm run check",
		"postbuild": "tsc",
		"test": "node --test",
		"check": "biome check --write",
		"tsc": "tsc --noEmit",
		"pretest": "npm run build",
		"test:ci": "node --test"
	},
	"peerDependencies": {
		"@pandacss/dev": ">=0.49.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@pandacss/dev": "0.49.0",
		"@swc/cli": "0.5.2",
		"@swc/core": "1.10.1",
		"@types/debug": "4.1.12",
		"@types/node": "^22.10.2",
		"typescript": "5.7.2"
	},
	"types": "dist/index.d.ts",
	"author": "Diego Stratta <diego@medici.uy>",
	"repository": "git@github.com:medici-uy/ui.git",
	"license": "AGPL-3.0-or-later",
	"engines": {
		"node": ">= 23.4.0"
	}
}
