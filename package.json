    "test": "node --test",
    "pretest": "npm run build",
    "test:ci": "node --test"
  },
  "dependencies": {
    "intl-messageformat": "10.5.14"
  },
  "peerDependencies": {
    "@pandacss/dev": ">=0.46.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.46.0",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.26",
    "@types/debug": "4.1.12",
    "prettier": "3.3.3",
    "typescript": "5.6.2"
  },
  "types": "dist/index.d.ts",
  "author": "Diego Stratta <diego@medici.uy>",
  "repository": "git@github.com:medici-uy/ui.git",
